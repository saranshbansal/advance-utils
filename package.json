{
  "name": "ept",
  "version": "1.0.0",
  "description": "A Gartner's tool for managing EXP engagement planning",
  "main": "index.js",
  "repository": "http://zeta.gartner.com/SSDTools/ept/trunk",
  "scripts": {
    "watch": "webpack --progress --colors --config ./src/main/webapp/app/webpack/webpack.local.config.js --display-cached --profile --watch",
    "prod": "npm run setup && webpack --progress --colors --config ./src/main/webapp/app/webpack/webpack.prod.config.js --display-cached --profile",
    "qa": "npm run setup && webpack --progress --colors --config ./src/main/webapp/app/webpack/webpack.qa.config.js --display-cached --profile ",
    "dev": "npm run setup && webpack --progress --colors --config ./src/main/webapp/app/webpack/webpack.dev.config.js --display-cached --profile",
    "local": "npm run setup && webpack --progress --colors --config ./src/main/webapp/app/webpack/webpack.local.config.js --display-cached --profile",
    "setup": "node ./src/main/webapp/app/tools/messages.js && npm install --no-optional",
    "test": "mocha src/**/test/config/setup.js src/**/*-test.js --reporter spec",
    "test:watch": "mocha --watch src/**/test/config/setup.js src/**/*-test.js --reporter spec"
  },
